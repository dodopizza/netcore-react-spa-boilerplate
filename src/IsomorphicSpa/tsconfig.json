{
	"compilerOptions": {
		"baseUrl": "./Client",
		"experimentalDecorators": true,
		"skipDefaultLibCheck": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"module": "es2015",
		"target": "es5",
		"jsx": "react",
		"lib": [
			"es6",
			"dom"
		],
		"types": [
			"mocha",
			"webpack-env"
		]
	},
	"exclude": [
		"node_modules",
		"wwwroot",
		"wwwroot/dist",
		"Client/dist"
	]
}